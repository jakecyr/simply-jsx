{
  "name": "simply-jsx",
  "version": "0.0.1",
  "description": "Simple transpiler for JSX without React",
  "main": "compile.js",
  "bin": "compile.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "node compile.js",
    "build": "babel -d dist src/**/*.jsx",
    "babel": "babel -d dist"
  },
  "keywords": [
    "jsx",
    "babel",
    "javascript"
  ],
