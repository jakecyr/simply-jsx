    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "cheerio": "^1.0.0-rc.11",
    "express": "^4.18.1",
    "node-watch": "^0.7.3",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^17.0.0",
    "fs-extra": "^10.1.0"
  }
